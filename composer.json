{
    "name": "aydin-hassan/vhost-parser-writer",
    "description": "VHost Parser to provide a simple API to modify Apache VHosts",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Aydin Hassan",
            "email": "aydin@hotmail.co.uk"
        }
    ],
    "require" : {
        "php" : ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.1",
        "squizlabs/php_codesniffer": "1.4.*",
        "satooshi/php-coveralls": "~0.6",
        "aydin-hassan/strip-indent": "1.0.0"
    },
    "autoload" : {
        "psr-4" : {
            "AydinHassan\\VHostParserWriter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": { "AydinHassan\\VHostParserWriterTest\\": "test/" }
    }
}
